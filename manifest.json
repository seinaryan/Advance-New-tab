{
  "manifest_version": 3,
  "name": "Custom New Tab Page",
  "version": "1.0",
  "description": "Replace the default new tab page with a custom page.",
  "author": "Your Name",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "bookmarks"
  ],
  "commands": {
    "focus-search": {
      "suggested_key": {
        "default": "Ctrl+Shift+L"
      },
      "description": "Focus the search input"
    }
  }
}
